{
    "name": "c1/c1-fluid-styled-svg",
    "type": "typo3-cms-extension",
    "description": "Better svg rendering with the f:media viewhelper. Registers a custom ImageRenderer for svg.",
    "keywords": [
        "TYPO3",
        "fluid",
        "fluid_styled_content",
        "svg",
        "images"
    ],
    "homepage": "https://github.com/mmunz/c1_fluid-styled-svg",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Manuel Munz",
            "email": "t3dev@comuno.net"
        }
    ],
    "require": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "typo3/cms-core": "^13.4"
    },
    "autoload": {
        "psr-4": {
            "C1\\FluidStyledSvg\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "c1_fluid_styled_svg"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
